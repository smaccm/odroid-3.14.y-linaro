	gsc_2: video-scaler@13e20000 {
		compatible = "samsung,exynos5-gsc";
		reg = <0x13e20000 0x1000>;
		interrupts = <0 87 0>;
		clocks = <&clock CLK_GSCL2>;
		clock-names = "gscl";
		samsung,power-domain = <&gsc_pd>;
	};

	gsc_3: video-scaler@13e30000 {
		compatible = "samsung,exynos5-gsc";
